	<script>
		user = {};
		game = {};

		x = -15;
		y = 0;

		$(function(){
			setInterval(function() {
				x = -1 * x;
				$(".meter > span").each(function() {
				$(this)
					.stop()
					.data("origWidth", $(this).width() / $(this).parent().width() * 100)
					.animate({
						width: ($(this).data("origWidth") - x - y) + "%"
					}, 1000);
			});
			}, 1000);
		});
	</script>
	
